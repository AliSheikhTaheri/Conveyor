  <package id="2" version="1.2.4" url="" name="Embedded Content" folder="6e12d1e4-7e48-4bc0-8eb6-622e822240d0" packagepath="" repositoryGuid="" packageGuid="" hasUpdate="False" enableSkins="False" skinRepoGuid="00000000-0000-0000-0000-000000000000">
